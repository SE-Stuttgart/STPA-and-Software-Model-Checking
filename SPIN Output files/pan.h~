#define Pp_main	((P17 *)this)
typedef struct P17 { /* p_main */
	unsigned _pid : 8;  /* 0..255 */
	unsigned _t   : 6; /* proctype */
	unsigned _p   : 9; /* state    */
#ifdef HAS_PRIORITY
	unsigned _priority : 8; /* 0..255 */
#endif
	uchar lck_id;
	int i;
/* XXX p=< p_initpid >, s=<p_main>, buf=<p_main > r->s->name=< pid * pid >XXX */
/* XXX p=< p_initacc >, s=<p_main>, buf=<p_main > r->s->name=< acc * acc >XXX */
